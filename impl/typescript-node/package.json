{
  "name": "@david-vectara/vectara-ts-node",
  "version": "0.0.22",
  "description": "This is the typescript (Node) implementation of the Vectara SDK for the OpenAPI V2 stubs. Please note, this has the\ndeprecated dependency on the \"request\" package due to the OpenAPI typescript-node generator.",
  "main": "./dist/index.js",
  "export" : {
    "api": "./dist/api.js"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc -p tsconfig-build.json",
    "clean": "tsc --build --clean",
    "build-bak": "tsup",
    "test": "jest --testPathPattern=tests",
    "coverage": "jest --testPathPattern=tests --collectCoverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/david-vectara/vectara-sdk.git"
  },
  "keywords": [
    "sdk",
    "vectara",
    "typescript"
  ],
  "author": "david@vectara.com",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/david-vectara/vectara-sdk/issues"
  },
  "homepage": "https://github.com/david-vectara/vectara-sdk#readme",
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-typescript": "^7.24.7",
    "@jest/globals": "^29.7.0",
    "@types/node": "^20.12.7",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "request": "2.88.2",
    "ts-jest": "^29.1.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "@badgateway/oauth2-client": "^2.3.0",
    "axios": "^1.6.8",
    "js-yaml": "^4.1.0",
    "request": "2.88.2"
  }
}
